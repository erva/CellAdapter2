            </intent-filter>
        </activity>
        <activity android:name="io.erva.celladapter2.base.BaseSampleActivity" />
        <activity android:name="io.erva.celladapter2.single.SingleChoiceActivity" />
        <activity android:name="io.erva.celladapter2.multi.MultiChoiceActivity" />
    </application>

</manifest>
